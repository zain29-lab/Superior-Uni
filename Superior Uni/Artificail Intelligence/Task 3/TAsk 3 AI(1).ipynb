   "source": [
    "# Task 3:Task 3: Sort text (word) in Alphabetical Order"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "6858999d-0b8f-4eb2-afcc-c741e225f11e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['apple', 'banana', 'cherry', 'orange']\n"
     ]
    }
   ],
   "source": [
    "words = [\"banana\", \"apple\", \"cherry\", \"orange\"]\n",
    "\n",
    "n = len(words)\n",
    "for i in range(n):\n",
    "    for j in range(0, n-i-1):\n",
    "        if words[j] > words[j+1]:\n",
    "            words[j], words[j+1] = words[j+1], words[j]\n",
    "\n",
    "print(words)"
   ]
